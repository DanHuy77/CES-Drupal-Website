{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}

{% if items.next %}
  <div class="case-studies__filters__pager">
    <div{{attributes}}>
      <div class="pager__item">
        <a class="button ces-btn load-more" href="{{ items.next.href }}" title="{{ 'Load more items'|t }}" rel="next">{{ options.button_text }}</a>
      </div>
    </div>
  </div>
{% endif %}
